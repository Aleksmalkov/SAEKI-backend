{
    "version": 2,
    "builds": [
      {
        "src": "vercel.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "vercel.ts",
        "headers": {
          "Access-Control-Allow-Origin": "https://saeki-lilac.vercel.app",
          "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      }
    ]
  }
  